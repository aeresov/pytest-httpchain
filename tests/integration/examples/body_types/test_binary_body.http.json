{
  "stages": [
    {
      "name": "test_binary_body_csv",
      "request": {
        "url": "https://httpbin.org/post",
        "method": "POST",
        "headers": {
          "Content-Type": "text/csv"
        },
        "body": {
          "binary": "tests/integration/examples/body_types/sample.csv"
        }
      },
      "response": [
        {
          "verify": {
            "status": 200
          }
        },
        {
          "save": {
            "jmespath": {
              "received_data": "data"
            }
          }
        },
        {
          "verify": {
            "expressions": [
              "{{ 'Alice' in received_data }}",
              "{{ 'Bob' in received_data }}",
              "{{ 'Charlie' in received_data }}"
            ]
          }
        }
      ]
    }
  ]
}
